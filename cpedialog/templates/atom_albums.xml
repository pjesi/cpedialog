<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<rss version="2.0" xmlns:media="http://search.yahoo.com/mrss/"
    xmlns:atom="http://www.w3.org/2005/Atom">
    <channel>
        {% for item in useralbums %}
         {% for album in item.1 %}
         <atom:link rel="{{album.gphoto_id.text}}" href="http://photos.googleapis.com/data/feed/base/user/{{item.0}}/albumid/{{album.gphoto_id.text}}?kind=photo&amp;imgmax=800" />
         {% endfor %}
        {% endfor %}
    </channel>
</rss>
