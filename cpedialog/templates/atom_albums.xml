<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<rss version="2.0" xmlns:media="http://search.yahoo.com/mrss/"
    xmlns:atom="http://www.w3.org/2005/Atom">
    <channel>
        {% for item in useralbums %}
         {% for album in item.1 %}
        <atom:link rel="alternate" href="http://picasaweb.google.com/data/feed/projection/user/{{item.0}}/albumid/{{album.name.text}}?kind=photo" />
         {% endfor %}
        {% endfor %}
    </channel>
</rss>
